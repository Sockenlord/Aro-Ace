    "tokenColors": [
        {
            "scope": [],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#3DA542"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder",
                "source"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Colors and Entities",
            "scope": [
                "constant.other.color",
                "punctuation.definition.entity",
                "constant.character.entity",
                "punctuation.definition.template-expression",
                "punctuation.section.tag.twig"
            ],
            "settings": {
                "foreground": "#62aedc"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Keyword, Storage, Constant",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape"
            ],
            "settings": {
                "foreground": "#62aedc",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.regexp",
                "string.interpolated"
            ],
            "settings": {
                "foreground": "#eccd00"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.numeric.css"
            ],
            "settings": {
                "foreground": "#e28c00"
            }
        },
        {
            "name": "HTML Tags",
            "scope": [
                "entity.name.tag.html",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#62aedc",
                "fontStyle": "bold"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#e28c00",
                "fontStyle": "italic"
            }
        },
        {
            "name": "JSON Properties",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#62aedc",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes and Types",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class",
                "support.type",
                "storage.type.cs"
            ],
            "settings": {
                "foreground": "#62aedc",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#62aedc",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#e28c00",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#eccd00",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#62aedc",
                "fontStyle": "underline"
            }
        }
    ]
}